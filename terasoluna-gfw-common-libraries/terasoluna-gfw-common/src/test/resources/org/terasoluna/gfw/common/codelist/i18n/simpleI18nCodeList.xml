  <bean id="CL_testFallbackToInvalidLanguage" class="org.terasoluna.gfw.common.codelist.i18n.SimpleI18nCodeList"
    lazy-init="true">
    <property name="rows">
      <util:map>
        <entry key="en">
          <util:map>
            <entry key="0" value="Sun." />
            <entry key="1" value="Mon." />
            <entry key="2" value="Tue." />
