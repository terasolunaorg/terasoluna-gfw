    </property>
  </bean>

  <bean id="CL_WEEK_EN" class="org.terasoluna.gfw.common.codelist.SimpleMapCodeList">
    <property name="map">
      <util:map>
        <entry key="0" value="Sun." />
        <entry key="1" value="Mon." />
        <entry key="2" value="Tue." />
        <entry key="3" value="Wed." />
